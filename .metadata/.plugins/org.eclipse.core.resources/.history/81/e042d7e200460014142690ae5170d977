

public class SoldOutState implements State {
    GumballMachine gumballMachine;
 
    public SoldOutState(GumballMachine gumballMachine) {
        this.gumballMachine = gumballMachine;
    }
 
    public void insertQuarter()
    {
    	System.out.println("You can not insert Quarter coin now ... Gumball Machine is in soldOut state.");
    }
	public void insertDime()
	{
		System.out.println("You can not insert Quarter coin now ... Gumball Machine is in soldOut state.");
	}
	public void insertNickel()
	{
		System.out.println("You can not insert Nikel coin now ... Gumball Machine is in soldOut state.");
	}
	public void turnCrank()
	{
		System.out.println("You can not turn crank now ... Gumball Machine is in soldOut state.");
	}
	public void dispense()
	{
		System.out.println("You can not dispense coin now ... Gumball Machine is in soldOut state.");
	}
	public void ejectCoins()
	{
		System.out.println("You can not eject Quarter coin now ... Gumball Machine is in soldOut state.");
	}
}
