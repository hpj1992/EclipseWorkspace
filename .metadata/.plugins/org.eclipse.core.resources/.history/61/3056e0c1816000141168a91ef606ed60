import java.util.ArrayList;


public class Cheese extends Decorator {
	
	
	
	
	
	 public Cheese(Component2 c)
	    {
	       super(c);
	       
	    }

	    
	     public double getPrice()
	    {
	       ArrayList<String>burgers = BuildOrder.getCheese();
	        double cost =0;
	        if(burgers.size()!=0)
	            {
	                cost = burgers.size()-1;
	            }
	         cost+=super.getCost();
	       
	    }

}
