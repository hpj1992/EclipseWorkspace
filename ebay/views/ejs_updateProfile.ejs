<!DOCTYPE html>
<html lang="en">

<head>
<script src="http://code.jquery.com/jquery-1.9.1.min.js" type="text/javascript">
</script>
<script type="text/javascript">
    $( document ).ready(function() {
        console.log( "document loaded" );
                 $("#header").load("header"); 
    });
 
    $( window ).load(function() {
        console.log( "window loaded" );
    });
</script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

 </head>

 <body>
 <div id="header">
 </div>
 <div style="margin-left:50px"> 

 <form action="UpdateProfile" method="post" id="updateProfile">
  <table>

<tr>
<td>
		 <label id="email">Email</label>
		 <input id="Email" value=<%=Email%> name="Email" type="hidden"> </input>
</td>
<td>
		 <label id="email" value=<%=Email%>><h5><%=Email%></h5></label>
</td>
</tr>
 <tr>
 	<td>
		<label for="firstname"> First Name </label>
	</td>
	<td>
		<input id="firstname" name="firstname" type="text"/>
	</td>
</tr>
<tr>
	<td>
		<label for="lastname"> Last Name </label>
	</td>
	<td>
		<input id="lastname" name="lastname" type="text"/>
	</td>
</tr>
<tr>
	<td>
		<label id="lbl_password" name="lbl_password">Password</label>
	</td>
	<td>
		<input id="password" name="password" type="password"/>
	</td>
</tr>
<tr>
	<td>
		<label for="lb1_address" id="lb1_address">Address 1</label>
	</td>
	<td>
	<input id="address" name="address" type="text"> 
	</td>
</tr>
	<tr>
	<td>
		<label for="lb1_address2" id="lb1_address2">Address 2</label>
	</td>
	<td>
	<input id="address2" name="address2" type="text"/> 
	</td>
</tr>

	<tr>
	<td>
		<label for="lb1_city" id="lb1_city">City</label>
	</td>
	<td>
	<input id="city" name="city" type="text"/> 
	</td>
</tr>



	<tr>
	<td>
		<label for="lbl_State" id="lbl_State">State</label>
	</td>
	<td>
	<input id="state" name="state" type="text"/> 

	</td>
</tr>


	<tr>
	<td>
		<label for="lbl_Phone" id="lbl_Phone">Phone</label>
	</td>
	<td>
	<input id="phone" name="phone" type="text" maxlength="10" /> 

	</td>
</tr>


	<tr>
	<td>
		<label for="lbl_zip" id="lbl_zip">Zip</label>
	</td>
	<td>
	<input id="zip" name="zip" type="text"/> 
	</td>
</tr>

	<tr>
	<td>
	<label for="country" id="country">Country</label>
	</td>
	<td>
	<input id="Country" name="Country" type="text"/> 
	</td>
</tr>
<tr>

</tr>


</table>
</br>
<input type="Submit" value="Update">
</form>
</div>
 </body>
 <script src="http://code.jquery.com/jquery.js"></script>
 <script type="text/javascript" src="scripts/gen_validatorv4.js">
</script> 
<script type="text/javascript">

var frmvalidator  = new Validator("updateProfile");
frmvalidator.addValidation("firstname","req","Please enter first name");
frmvalidator.addValidation("firstname","alphabetic","Numbers not allowed in first name");
frmvalidator.addValidation("lastname","req","Please enter last name");
frmvalidator.addValidation("password","req","Please enter password");

frmvalidator.addValidation("firstname","maxlen=20","Maximum length is 80 charcter in first name");
frmvalidator.addValidation("lastname","maxlen=20","Maximum length is 80 charcter in last name");

frmvalidator.addValidation("zip","numeric","Please enter numeric zip code");
frmvalidator.addValidation("zip","maxlen=5","Please enter valid zip code");
frmvalidator.addValidation("zip","minlen=5","Please enter valid zip code");


frmvalidator.addValidation("Country","alphabetic","Please enter valid country");
frmvalidator.addValidation("state","alphabetic","Please enter valid state");
frmvalidator.addValidation("city","alphabetic","Please enter valid city");


frmvalidator.addValidation("address","maxlen=30","Please addreess less than 50 characters");
frmvalidator.addValidation("address2","maxlen=30","Please addreess less than 50 characters");
</script>
<script type="text/javascript">
$(document).ready(function()
{
 $.ajax({
    type: 'get',
    contentType: 'application/json',
    url: '/GetProfile?Email='+$('#Email').val(),
                        success: function(data) {
                 					$('#firstname').val(data[0].email);
                 					$('#lastname').val(data[0].lastName);
                 					$('#firstname').val(data[0].firstName);
                 					$('#address').val(data[0].address1);
                 					$('#address2').val(data[0].address2);
                 					$('#city').val(data[0].city);
                 					$('#state').val(data[0].state);
                 					$('#zip').val(data[0].zipcode);
                 					$('#phone').val(data[0].phone);
                 					$('#password').val(data[0].password);
                 					$('#Country').val(data[0].country);
                }
            
        });
                                                    
   
});

 </script>
 </html>


