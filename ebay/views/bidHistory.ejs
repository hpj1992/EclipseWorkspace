														{
														
														alert('Congrates');
															console.log("Congrates perfect bid ");
															var placedBid = parseFloat(jQuery('#MaxBidId').val())
															var nextmin = placedBid + ((placedBid * 1)/ 100)
															jQuery('#bidAlert').html("");
																	jQuery.ajax({type:'POST',
																	 url:'/postBid',
																	 data : {auctionId : <%=auctionId%> , bidPrice : placedBid},
																	 success:function(data,textStatus){
																	 
																							jQuery('#MaxBidId').val('');
																							if(data.result == "true"){
																							refreshBids();
																							jQuery('#bidAlert').html("");
																							}
																							else if(data.result == "false"){
																							jQuery('#bidAlert').html("Error while processing your bid");  
																							}											
																					},
																	 error:function(XMLHttpRequest,textStatus,errorThrown){
																	 console.log('In error')
																	 }});	
														}
														else
														{
														jQuery('#bidAlert').html("Please enter proper value");									 		
														}
													}
													}
												}
												},
														 error:function(XMLHttpRequest,textStatus,errorThrown){
														 console.log('In error')
														 }
														 });				 
	}); 
 });
 
