			 jQuery.ajax({type:'GET',
						 url:'/refreshBids?auctionId=<%=auctionId%>', 
						 success:function(data,textStatus){								 												
											if(data.results == "false"){
											
											alert('False1');
											
											}
											else if(data.results == "true"){	
											
											alert('True1');
											
											nextMinimum = data.currentBid + ((data.currentBid * 1)/ 100);
											
											jQuery('#nextMinimumBid').html("(Enter US $"+nextMinimum + " or more)");
											jQuery('#totalBidders').html(data.totalUsers);
											jQuery('#currentBid').html("US $"+data.currentBid);											
											jQuery('#totalBids').html(data.totalBids);
											
											Number.prototype.padLeft = function(base,chr)
											{
												var  len = (String(base || 10).length - String(this).length)+1;
												return len > 0? new Array(len).join(chr || '0')+this : this;
											}
											
											for(i = 0 ; i < data.bids.length ; i++)
											{										
											jQuery('#userName'+i).html(data.bids[i].userName);
											jQuery('#bidAmount'+i).html("US $"+data.bids[i].bidPrice);
											var dateBid = new Date(data.bids[i].date);
											
											dateBid = [dateBid.getFullYear(),(dateBid.getMonth()+1).padLeft(),
														  dateBid.getDate().padLeft()
															].join('/') +' ' +
															[dateBid.getHours().padLeft(),
															 dateBid.getMinutes().padLeft(),
															 dateBid.getSeconds().padLeft()].join(':');
											jQuery('#bidDate'+i).html(dateBid);
											}										
											}
									},
						 error:function(XMLHttpRequest,textStatus,errorThrown){
						 console.log('In error')
						 }
						 });		
		}	
		$( window ).load(function() {
		  refreshBids();
		});
			
		$("#refreshLink").click(function() {        
			refreshBids();				 
		});
	
